{
  "name": "@fitvibe/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.json && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src\" \"../../tests/frontend\" --max-warnings=0",
    "lint:check": "turbo run lint -- --max-warnings=0",
    "typecheck": "tsc --noEmit && node -e \"const fs=require('fs');const path=require('path');const testsDir=path.join(__dirname,'tests');if(fs.existsSync(testsDir)){const files=fs.readdirSync(testsDir,{recursive:true});if(files.some(f=>/\\\\.(ts|tsx)$/.test(f))){require('child_process').execSync('tsc --noEmit -p tsconfig.test.json',{stdio:'inherit'});}}\"",
    "test": "node ./scripts/run-vitest.mjs",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.7",
    "axios": "^1.7.2",
    "i18next": "^23.10.1",
    "lucide-react": "^0.548.0",
    "preact": "^10.23.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.27.0",
    "recharts": "^2.12.7",
    "zustand": "^4.5.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.19",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-istanbul": "^2.1.4",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.4"
  }
}
